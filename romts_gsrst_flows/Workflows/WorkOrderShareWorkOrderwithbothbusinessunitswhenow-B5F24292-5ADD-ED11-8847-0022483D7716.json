            ]
          },
          "metadata": {
            "operationMetadataId": "abe6c6dd-be95-454c-9321-62fd565b3be4"
          },
          "type": "InitializeVariable",
          "inputs": {
            "variables": [
              {
                "name": "TeamId",
                "type": "string"
              }
            ]
          }
        }
      }
    },
    "templateName": ""
  },
  "schemaVersion": "1.0.0.0"
}